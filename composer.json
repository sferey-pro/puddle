{
    "name": "sferey/puddle",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4.7",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mongodb": "*",
        "api-platform/doctrine-odm": "^4.1",
        "api-platform/doctrine-orm": "^4.1",
        "api-platform/symfony": "^4.1",
        "babdev/pagerfanta-bundle": "^4.5",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/mongodb-odm-bundle": "^5.3",
        "doctrine/orm": "^3.3",
        "dragonmantank/cron-expression": "^3.4",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "league/oauth2-github": "^3.1",
        "league/oauth2-google": "^4.0",
        "nelmio/cors-bundle": "^2.5",
        "pagerfanta/doctrine-mongodb-odm-adapter": "^4.7",
        "pagerfanta/doctrine-orm-adapter": "^4.7",
        "pagerfanta/twig": "^4.7",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.1",
        "runtime/frankenphp-symfony": "^0.2.0",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/amqp-messenger": "^7",
        "symfony/asset": "^7",
        "symfony/asset-mapper": "^7",
        "symfony/cache": "^7",
        "symfony/clock": "^7",
        "symfony/console": "^7",
        "symfony/doctrine-messenger": "^7",
        "symfony/dotenv": "^7",
        "symfony/expression-language": "^7",
        "symfony/flex": "^2",
        "symfony/form": "^7",
        "symfony/framework-bundle": "^7",
        "symfony/intl": "^7",
        "symfony/mailer": "^7",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "^7",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "symfony/runtime": "^7",
        "symfony/scheduler": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/stimulus-bundle": "^2.24",
        "symfony/translation": "^7",
        "symfony/twig-bundle": "^7",
        "symfony/ux-icons": "^2.24",
        "symfony/ux-live-component": "^2.24",
        "symfony/ux-toggle-password": "^2.24",
        "symfony/ux-turbo": "^2.25",
        "symfony/ux-twig-component": "^2.24",
        "symfony/validator": "^7",
        "symfony/workflow": "^7",
        "symfony/yaml": "^7",
        "symfonycasts/sass-bundle": "^0.8",
        "symfonycasts/verify-email-bundle": "^1.17",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.21",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "DataFixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7",
            "docker": true
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "phpunit/phpunit": "^12",
        "symfony/maker-bundle": "^1.63",
        "symfony/panther": "^2.2",
        "symfony/stopwatch": "^7",
        "symfony/web-profiler-bundle": "^7",
        "zenstruck/browser": "^1.9",
        "zenstruck/foundry": "^2"
    }
}
