<td>{{ raw_material.name }}</td>
<td>{{ raw_material.unitPrice }}</td>
<td>{{ raw_material.supplier }}</td>
<td>{{ raw_material.priceVariability ? ux_icon('material-symbols:check-circle') : ux_icon('material-symbols:check-cancel') }}</td>
<td>{{ raw_material.unit }}</td>
<td>{{ raw_material.totalCost }}</td>
<td>{{ raw_material.notes }}</td>
<td><twig:Semantic:ExternalLink content="{{ 'action.external_link'|trans }}" link="{{ raw_material.link }}" /></td>
<td>
    <twig:Button:ActionItem
        withShow="app_raw_material_show"
        withEdit="app_raw_material_edit"
        withDelete="app_raw_material_delete"
        :itemId="raw_material.id"
    />
</td>
